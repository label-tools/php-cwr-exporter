{
    "name": "label-tools/php-cwr-exporter",
    "description": "Modular Common Works Registration (CWR) exporter for PHP",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LabelTools\\PhpCwrExporter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Julian Castaneda",
            "email": "juliancc@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0 || ^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
            "LabelTools\\PhpCwrExporter\\Support\\CwrServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
